            RowBox[{
             RowBox[{"-", "\[Beta]"}], " ", "Hamiltonian"}], "/.", " ", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"s", "[", "k", "]"}], "->", 
