Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PartitionFunctionForIsingSpins", "[", 
    RowBox[{"Hamiltonian_", ",", "n_Integer", ",", 
     RowBox[{"OptionsPattern", "[", 
      RowBox[{"{", 
       RowBox[{"\"\<Verbose\>\"", "->", "False"}], "}"}], "]"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"spins", ",", "Zsym", ",", "Zval", ",", "configs", ",", 
       RowBox[{"i", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Define", " ", "the", " ", "spins", " ", "as", " ", 
       RowBox[{"s", "[", "i", "]"}], " ", "symbols"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"spins", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"s", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Construct", " ", "symbolic", " ", "Boltzmann", " ", "factor"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Zsym", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "\[Beta]"}], "*", "Hamiltonian"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"OptionValue", "[", "\"\<Verbose\>\"", "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<> Symbolic partition function: Z = \>\"", ",", "Zsym"}], "]"}],
          ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Generate", " ", "all", " ", "spin", " ", 
        RowBox[{"configurations", ":", 
         RowBox[{
          RowBox[{"each", " ", 
           RowBox[{"s", "[", "i", "]"}]}], "\[Element]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "1"}], ",", "1"}], "}"}]}]}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"configs", "=", 
       RowBox[{"Tuples", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "n"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Sum", " ", "the", " ", "Boltzmann", " ", "factor", " ", "over", " ", 
        "all", " ", "configurations"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Zval", "=", 
       RowBox[{"Total", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"i", "++"}], ";", "\[IndentingNewLine]", 
            RowBox[{"If", "[", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"OptionValue", "[", "\"\<Verbose\>\"", "]"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{"Print", "[", 
               RowBox[{
               "\"\<> Summing configuration #\>\"", ",", "i", ",", 
                "\"\<: \>\"", ",", "#"}], "]"}]}], "\[IndentingNewLine]", 
             "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"Zsym", "/.", " ", 
             RowBox[{"Thread", "[", 
              RowBox[{"spins", "->", "#"}], "]"}]}]}], ")"}], "&"}], "/@", 
         "configs"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Return", " ", "final", " ", "result"}], "*)"}], 
      "\[IndentingNewLine]", "Zval"}]}], "]"}]}], "\n"}]], "Input",
