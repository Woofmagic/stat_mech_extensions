          RowBox[{"tau", ">", "0"}]}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tau", ">", "0"}], "&&", 
        RowBox[{"disc", ">", "0"}]}], ",", "\"\<Unstable Node\>\"", ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tau", ">", "0"}], "&&", 
        RowBox[{"disc", "==", "0"}]}], ",", 
