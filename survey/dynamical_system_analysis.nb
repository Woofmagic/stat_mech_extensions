       RowBox[{"4", "*", "delta"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Which", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"delta", "<", "0"}], ",", "\"\<Saddle Point\>\"", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"delta", "==", "0"}], ",", 
       "\"\<Non-isolated (at least one zero eigenvalue)\>\"", ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Stable", " ", 
         RowBox[{"cases", ":", 
          RowBox[{"tau", "<", "0"}]}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tau", "<", "0"}], "&&", 
        RowBox[{"disc", ">", "0"}]}], ",", "\"\<Stable Node\>\"", ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tau", "<", "0"}], "&&", 
        RowBox[{"disc", "==", "0"}]}], ",", "\"\<Degenerate Stable Node\>\"", 
       ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"tau", "<", "0"}], "&&", 
        RowBox[{"disc", "<", "0"}]}], ",", "\"\<Stable Spiral\>\"", ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
